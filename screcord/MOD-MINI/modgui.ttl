@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<https://github.com/brummer10/screcord#mono_record_mini>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-monorecord_mini.html> ;
        modgui:stylesheet <modgui/stylesheet-monorecord_mini.css> ;
        modgui:screenshot <modgui/screenshot-monorecord_mini.png> ;
        modgui:thumbnail <modgui/thumbnail-monorecord_mini.png> ;
        modgui:javascript <modgui/script-screcord_mini.js> ;
        modgui:brand "brummer" ;
        modgui:label "Record Mono Mini" ;
        modgui:model "sc_record_mini" ;
        modgui:monitoredOutputs [ lv2:symbol "CLIP" ] ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "FORMAT" ;
            lv2:name "FORMAT" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "REC" ;
            lv2:name "REC" ;
        ] ;
    ] .


